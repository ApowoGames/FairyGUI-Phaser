    "dts": "dts-bundle --configJson dts-bundle.json && rm -rf types"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "circular-dependency-plugin": "^5.2.2",
    "dts-bundle": "^0.7.3",
    "file-loader": "^6.2.0",
    "genversion": "^2.2.0",
    "phaser3": "git+ssh://git@code.apowo.com:opensource/phaser3.git",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-loader": "^7.0.5",
    "tslib": "^2.3.1",
    "typescript": "^3.9.6"
  }
}
